#include<stdio.h>
#include<conio.h>

int lcm();
int main()
{
	lcm();
	return 0;
}

int lcm()
{
	int a,b,c;
	printf("input first number:");
	scanf("%d",&a);
	printf("input second number:");
	scanf("%d",&b);
	c=(a>b)?a:b;
	
	while(true)
	{
		if(c%a==0 && c%b==0)
		{
			printf("the LCM of %d and %d is %d",a,b,c);
			break;
		}
		c++;
	}
	return 0;
}
